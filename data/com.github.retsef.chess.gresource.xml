    <file alias="QueenW.png" compressed="true">assets/pieces/QueenW.png</file>
    <file alias="RookB.png" compressed="true">assets/pieces/RookB.png</file>
    <file alias="RookW.png" compressed="true">assets/pieces/RookW.png</file>
    <file alias="KnightB.png" compressed="true">assets/pieces/KnightB.png</file>
    <file alias="KnightW.png" compressed="true">assets/pieces/KnightW.png</file>
