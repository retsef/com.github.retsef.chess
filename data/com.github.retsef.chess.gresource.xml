<gresources>
  <gresource prefix="/com/github/retsef/chess">
    <file alias="chess.css" compressed="true">chess.css</file>
    <file alias="chess.ui" compressed="true" preprocess="xml-stripblanks">chess.ui</file>
  </gresource>

  <gresource prefix="/com/github/retsef/chess/widgets">
    <file alias="board_box_widget.ui" compressed="true" preprocess="xml-stripblanks">widgets/board_box_widget.ui</file>
    <file alias="board_widget.ui" compressed="true" preprocess="xml-stripblanks">widgets/board_widget.ui</file>
  </gresource>

  <gresource prefix="/com/github/retsef/chess/backgrounds">
